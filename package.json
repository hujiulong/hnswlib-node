{
  "name": "hnswlib-node-prebuilt",
  "version": "2.0.0",
  "description": "Node.js bindings for Hnswlib",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "http://github.com/hujiulong/hnswlib-node.git"
  },
  "homepage": "https://github.com/hujiulong/hnswlib-node",
  "author": "Atsushi Tatsuma",
  "license": "Apache-2.0",
  "keywords": [
    "hnswlib",
    "approximate nearest neighbor search",
    "machine learning"
  ],
  "gypfile": true,
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "prepare": "husky install",
    "test": "jest",
    "doc": "typedoc",
    "binary:pack": "node-pre-gyp package",
    "binary:release": "node-pre-gyp-github publish --release"
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.10",
    "bindings": "^1.5.0",
    "node-addon-api": "^7.0.0",
    "node-pre-gyp-github": "^1.4.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/node": "^20.1.3",
    "eslint": "^8.10.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-extended": "^4.0.0",
    "typedoc": "^0.24.1",
    "typescript": "^5.0.4",
    "yarn-release": "^1.10.3"
  },
  "binary": {
    "module_name": "addon",
    "module_path": "./binary",
    "host": "https://github.com/hujiulong/hnswlib-node/releases/download/",
    "remote_path": "v{version}",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz"
  }
}
